<UserControl
	xmlns="https://github.com/avaloniaui"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:vm="using:Mesen.Debugger.ViewModels"
	mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="110"
	VerticalAlignment="Stretch"
	HorizontalAlignment="Stretch"
	x:DataType="vm:NesPpuViewModel"
	x:Class="Mesen.Debugger.Views.NesPpuView"
>
	<Design.DataContext>
		<vm:NesPpuViewModel />
	</Design.DataContext>

	<DockPanel>
		<StackPanel Orientation="Horizontal">
			<TextBlock>Cycle:</TextBlock>
			<TextBox Classes="h1" Text="{CompiledBinding Cycle}" />
			<TextBlock>Scanline:</TextBlock>
			<TextBox Classes="h1" Text="{CompiledBinding Scanline}" />
		</StackPanel>
	</DockPanel>
</UserControl>
